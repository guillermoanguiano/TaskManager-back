#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules/ts-node/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/node_modules:/mnt/c/Users/Memo/OneDrive/node_modules:/mnt/c/Users/Memo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Memo/onedrive/desktop/proyectos/CRUD-ts/backend-crud/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules/ts-node/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules/.pnpm/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/backend-CRUD/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/CRUD-Ts/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/Proyectos/node_modules:/mnt/c/Users/Memo/OneDrive/Desktop/node_modules:/mnt/c/Users/Memo/OneDrive/node_modules:/mnt/c/Users/Memo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Memo/onedrive/desktop/proyectos/CRUD-ts/backend-crud/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules/ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.2_@types+node@20.11.28_typescript@5.4.2/node_modules/ts-node/dist/bin-cwd.js" "$@"
fi
